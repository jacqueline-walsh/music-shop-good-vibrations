<p id="notice"><%= notice %></p>

<h1>Checkouts</h1>

<table>
  <thead>
    <tr>
      <th>Street</th>
      <th>Town</th>
      <th>County</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @checkouts.each do |checkout| %>
      <tr>
        <td><%= checkout.street %></td>
        <td><%= checkout.town %></td>
        <td><%= checkout.county %></td>
        <td><%= checkout.country %></td>
        <td><%= link_to 'Show', checkout %></td>
        <td><%= link_to 'Edit', edit_checkout_path(checkout) %></td>
        <td><%= link_to 'Destroy', checkout, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checkout', new_checkout_path %>
